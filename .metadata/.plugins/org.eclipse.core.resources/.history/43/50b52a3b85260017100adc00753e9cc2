
public class RPNParserException extends Exception{
	  
	private static final long serialVersionUID = 1L;

	/*public static void main(String args[]) {
	   myException('p'); }*/
	   
	public static void  myException (char parm)  {
		   if (Character.isLetter(parm) || Character.isSpaceChar(parm))
			   System.out.println("You've put character " + parm + ", but we can use only numbers or arithmetic signs");
		   } 
	   
}