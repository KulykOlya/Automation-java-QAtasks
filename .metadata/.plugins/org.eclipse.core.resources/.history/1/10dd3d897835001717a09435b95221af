package quadraticEquatation;

public class Solving {
	
	public static double[] solvingEquatution (double a, double b, double c, double D){
		
		double[] resultArray = new double[3];
		double x1;
		double x2;
		
		x1 = (-b + Math.sqrt(D))/(2*a);
		x2 = (-b - Math.sqrt(D))/(2*a);
		resultArray[0] = x1;
		resultArray[1] = x2;
		return resultArray;
	}
	
	public static double solvingEquatution (double a, double b){
		double x1 = -b/(2.0*a);
		return x1;}
}